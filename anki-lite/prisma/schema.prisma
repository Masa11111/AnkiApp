generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Card {
  id           Int      @id @default(autoincrement())
  front        String
  back         String
  intervalDays Int      @default(0)
  nextReviewAt DateTime
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt
}
